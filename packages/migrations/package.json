{
  "name": "migrations",
  "version": "0.1.0",
  "license": "AGPL-3.0",
  "contributors": [
    "Anton Ã–berg <oberg.anton@gmail.com>",
    "Fredrik Olovsson <fogithub@gmail.com>",
    "Ida Li <ida.li@nrm.se>"
  ],
  "scripts": {
    "db:development": "NODE_ENV=development DEBUG='DINA*' node_modules/.bin/sequelize",
    "db:development:create": "npm run db:development db:create",
    "db:development:drop": "npm run db:development db:drop",
    "db:development:import:fromJson": "IMPORT_DATA_NUMBER_OF_SPECIMENS=1000000 NODE_ENV=development DEBUG='DINA*,-DINA:LOG_DEBUG*' node ./src/import/fromJson.js",
    "db:development:migrate:latest": "npm run db:development db:migrate",
    "db:development:migrate:one": "npm run db:development db:migrate:one",
    "db:development:migrate:undo:all": "npm run db:development db:migrate:undo:all",
    "db:development:migrate:undo:one": "npm run db:development db:migrate:undo",
    "db:production": "NODE_ENV=production DEBUG='DINA*' node_modules/.bin/sequelize",
    "db:production:create": "npm run db:production db:create",
    "db:production:drop": "npm run db:production db:drop",
    "db:production:import:fromJson": "IMPORT_DATA_NUMBER_OF_SPECIMENS=1000000 NODE_ENV=production DEBUG='DINA*,-DINA:LOG_DEBUG*' node ./src/import/fromJson.js",
    "db:production:migrate:latest": "npm run db:production db:migrate",
    "db:production:migrate:one": "npm run db:production db:migrate:one",
    "db:production:migrate:undo:all": "npm run db:production db:migrate:undo:all",
    "db:production:migrate:undo:one": "npm run db:production db:migrate:undo",
    "db:test": "NODE_ENV=test DEBUG='DINA*,-DINA:DEBUG*' node_modules/.bin/sequelize",
    "db:test:create": "npm run db:test db:create",
    "db:test:drop": "npm run db:test db:drop",
    "db:test:import:fromJson": "IMPORT_DATA_NUMBER_OF_SPECIMENS=1000000 NODE_ENV=test DEBUG='DINA*,-DINA:LOG_DEBUG*' node ./src/import/fromJson.js",
    "db:test:migrate:latest": "npm run db:test db:migrate",
    "db:test:migrate:one": "npm run db:test db:migrate:one",
    "db:test:migrate:undo:all": "npm run db:test db:migrate:undo:all",
    "db:test:migrate:undo:one": "npm run db:test db:migrate:undo",
    "elasticsearch:development:rebuild-indices": "IMPORT_DATA_NUMBER_OF_SPECIMENS=1000000 NODE_ENV=development DEBUG='DINA*,-DINA:LOG_DEBUG*' node ./src/rebuildIndex/searchIndices.js",
    "elasticsearch:production:rebuild-indices": "IMPORT_DATA_NUMBER_OF_SPECIMENS=1000000 NODE_ENV=production DEBUG='DINA*,-DINA:LOG_DEBUG*' node ./src/rebuildIndex/searchIndices.js",
    "lint:js": "eslint 'src/**/*.js'",
    "lint:js:fix": "eslint 'src/**/*.js' --fix",
    "pretty": "prettier --write 'src/**/*.{js,jsx,json}'",
    "setup:development": "yarn db:development:drop ; yarn db:development:create && yarn db:development:migrate:latest && yarn db:development:import:fromJson",
    "setup:production": "yarn db:production:drop ; yarn db:production:create && yarn db:production:migrate:latest && yarn db:production:import:fromJson",
    "setup:test": "yarn db:test:drop ; yarn db:test:create && yarn db:test:migrate:latest && yarn db:test:import:fromJson",
    "test": "npm run test:lint && npm run test:unit",
    "test:depcheck": "depcheck . --ignores='pg,sequelize'",
    "test:lint": "npm run lint:js",
    "test:unit": "NODE_ENV=test jest --env=node",
    "test:unit:watch": "NODE_ENV=test jest --env=node --watch"
  },
  "dependencies": {
    "backend": "file:../backend",
    "common": "file:../common",
    "dotenv": "^4.0.0",
    "pg": "^7.4.3",
    "sequelize": "^5.15.1",
    "sequelize-cli": "^5.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "depcheck": "^0.7.2",
    "eslint": "4.17.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "jest": "23.6.0",
    "prettier": "1.16.4"
  },
  "engines": {
    "node": "10.x",
    "npm": "5.5.1"
  }
}
