  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "npm run build-css && npm run markdown:build && REACT_APP_ENABLE_MOCK=false react-scripts build",
    "build-css": "node-less-chokidar src/ -o src/",
    "build-snippets": "node ./src/scripts/snippets/index.js",
    "build:mock": "npm run build-css && npm run markdown:build && REACT_APP_ENABLE_MOCK=true react-scripts build",
    "build:storybook": "npm run update-storybook-imports && build-storybook -c .storybook -o build-storybook",
    "build:trees": "node ./src/scripts/buildFileStructureTrees.js",
    "eject": "react-scripts eject",
    "ensure-no-diff": "git diff --exit-code",
    "lint:js": "eslint 'src/**/*.js'",
    "lint:js:fix": "eslint 'src/**/*.js' --fix",
    "markdown:build": "node src/scripts/markdown/buildRenderedMarkdownFiles.js",
    "markdown:watch": "node src/scripts/markdown/watchMarkdownFiles.js",
    "mount:admin": "MOUNT_APP=admin node ./src/scripts/app/mountApp.js",
    "mount:collectionsUi": "MOUNT_APP=collectionsUi node ./src/scripts/app/mountApp.js",
    "pretty": "prettier --single-quote --write 'src/**/*.{js,jsx,json}'",
    "start": "NODE_PATH=./src/ npm-run-all -p watch-css markdown:watch start-js",
    "start-js": "react-scripts start",
    "start:mock": "REACT_APP_ENABLE_MOCK=true npm start",
    "storybook": "npm run update-storybook-imports && start-storybook -p 9009 -s public",
    "test": "npm run update-storybook-imports && npm run lint:js && react-scripts test",
    "test:coverage": "npm test -- --coverage",
    "test:unit:fast": "REACT_APP_TEST_UI=false react-scripts test --watch",
    "test:unit:watch": "react-scripts test --watch",
    "update-git-dependencies": "npm update models && npm update semantic-ui",
    "update-storybook-imports": "node ./src/scripts/storybook/createStorybookImports",
    "watch-css": "npm run build-css && node-less-chokidar src/ -o src/ --watch --recursive"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.1.2",
