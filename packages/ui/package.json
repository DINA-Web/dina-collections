  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "yarn build-css && yarn markdown:build && REACT_APP_ENABLE_MOCK=false react-scripts build",
    "build-css": "node-less-chokidar src/ -o src/",
    "build-snippets": "node ./src/scripts/snippets/index.js",
    "build:mock": "yarn build-css && yarn markdown:build && REACT_APP_ENABLE_MOCK=true react-scripts build",
    "build:trees": "node ./src/scripts/buildFileStructureTrees.js",
    "eject": "react-scripts eject",
    "ensure-no-diff": "git diff --exit-code",
    "lint:js": "eslint '**/*.js'",
    "lint:js:fix": "eslint '**/*.js' --fix",
    "markdown:build": "node src/scripts/markdown/buildRenderedMarkdownFiles.js",
    "markdown:watch": "node src/scripts/markdown/watchMarkdownFiles.js",
    "mount:admin": "MOUNT_APP=admin node ./src/scripts/app/mountApp.js",
    "mount:collectionsUi": "MOUNT_APP=collectionsUi node ./src/scripts/app/mountApp.js",
    "pretty": "prettier --write 'src/**/*.{js,jsx,json}'",
    "start": "NODE_PATH=./src/ npm-run-all -p watch-css markdown:watch start-js",
    "start-js": "react-scripts start",
    "start:mock": "REACT_APP_ENABLE_MOCK=true yarn start",
    "test": "yarn lint:js && yarn test:unit",
    "test:coverage": "yarn test:unit --coverage",
    "test:unit": "react-scripts test",
    "test:unit:fast": "REACT_APP_TEST_UI=false react-scripts test --watch",
    "test:unit:watch": "react-scripts test --watch",
    "test:e2e": "yarn test:e2e:headless",
    "test:e2e:watch": "yarn test:e2e:headful",
    "test:e2e:headful": "yarn cypress open",
    "test:e2e:headless": "yarn cypress run",
    "update-git-dependencies": "yarn upgrade models && yarn upgrade semantic-ui",
    "watch-css": "yarn build-css && node-less-chokidar src/ -o src/ --watch --recursive"
  },
  "devDependencies": {
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "cypress": "^3.1.5",
    "deep-freeze": "^0.0.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "jest-localstorage-mock": "^2.0.1",
    "node-less-chokidar": "^0.1.2",
    "npm-run-all": "^4.1.1",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^16.2.0",
    "source-map-explorer": "^1.5.0",
    "wait-on": "^3.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": "10.15.1",
    "yarn": "1.10.1"
  }
