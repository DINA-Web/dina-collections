  "scripts": {
    "build": "rm -rf es5 && babel ./src -d es5 --copy-files && node ./src/schemaBuilder/buildModels.js && node ./src/schemaBuilder/buildOpenApi.js && npm run test:unit",
    "build:js": "rm -rf es5 && babel ./src -d es5 --copy-files && npm run test:unit",
    "build:schema": "node ./src/schemaBuilder/buildModels.js && node ./src/schemaBuilder/buildOpenApi.js && npm run test:unit",
    "build:trees": "node ./src/scripts/buildFileStructureTrees.js",
    "lint:js": "eslint 'src/**/*.js'",
    "lint:js:fix": "eslint 'src/**/*.js' --fix",
    "pretty": "prettier --single-quote --write 'src/**/*.{js,jsx,json}'",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "npm run lint:js",
    "test:unit": "NODE_ENV=test jest --env=node",
    "test:unit:watch": "NODE_ENV=test jest --env=node --watch"
  },
  "dependencies": {
    "ajv": "^5.2.3",
    "debug": "^3.1.0",
    "json-schema-faker": "^0.4.4",
    "lodash": "^4.17.5",
    "moment": "^2.22.2",
    "normalizr": "^3.2.4",
    "object-path": "^0.11.4",
    "object-path-immutable": "^1.0.1",
    "performance-now": "^2.1.0",
    "redux": "^3.7.2",
    "reselect": "^3.0.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
