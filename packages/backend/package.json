  "dependencies": {
    "body-parser": "^1.18.2",
    "common": "file:../common",
    "debug": "^3.1.0",
    "deep-diff": "^0.3.8",
    "depcheck": "^0.6.8",
    "dotenv": "^4.0.0",
    "elasticsearch": "^15.0.0",
    "express": "^4.16.2",
    "isomorphic-fetch": "^2.2.1",
    "json2csv": "^4.2.1",
    "keycloak-admin-client": "^0.12.0",
    "keycloak-connect": "^3.4.3",
    "left-pad": "^1.3.0",
    "object-path": "^0.11.4",
    "object-path-immutable": "^3.0.0",
    "performance-now": "^2.1.0",
    "pg": "^7.4.3",
    "reselect": "^3.0.1",
    "response-time": "^2.3.2",
    "sequelize": "^4.38.0",
    "swagger-ui-dist": "^3.19.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^22.1.4",
    "nodemon": "^1.14.12",
    "prettier": "1.8.2"
  },
  "scripts": {
    "build:trees": "node ./src/scripts/buildFileStructureTrees.js",
    "data:clear": "NODE_ENV=development DEBUG='' IMPORT_DATA=false node src/apps/development/data/index.js",
    "data:load": "NODE_ENV=development DEBUG='DINA*, -DINA:LOG_DEBUG:controller*' IMPORT_DATA=true IMPORT_DATA_NUMBER_OF_SPECIMENS=1000 node src/apps/development/data/index.js",
    "data:loadAll": "NODE_ENV=development DEBUG='DINA*, -DINA:LOG_DEBUG:controller*' IMPORT_DATA=true IMPORT_DATA_NUMBER_OF_SPECIMENS=100000 node src/apps/development/data/index.js",
    "lint:js": "eslint 'src/**/*.js'",
    "lint:js:fix": "eslint 'src/**/*.js' --fix",
    "pretty": "prettier --single-quote --write 'src/**/*.{js,jsx,json}'",
    "production:data:clear": "NODE_ENV=production DEBUG='DINA*' node src/apps/production/data/index.js",
    "production:data:load": "NODE_ENV=production DEBUG='DINA:LOG_INFO*' IMPORT_DATA=true node src/apps/production/data/index.js",
    "production:data:loadAll": "NODE_ENV=production DEBUG='DINA*' IMPORT_DATA=true IMPORT_DATA_NUMBER_OF_SPECIMENS=100000 node src/apps/production/data/index.js",
    "production:start:api": "NODE_ENV=production DEBUG='DINA*' node src/apps/production/api/index.js",
    "production:start:worker": "NODE_ENV=production DEBUG='DINA*' node src/apps/production/worker/index.js",
    "start": "NODE_ENV=development DEBUG='DINA*,-DINA:LOG_INFO:lib/jobs/worker, -DINA:LOG_DEBUG:controller/jobGetMany' nodemon src/apps/development/api/index.js",
    "start:inspect": "NODE_ENV=development DEBUG='DINA*,-DINA:LOG_INFO:lib/jobs/worker' node --inspect src/apps/development/api/index.js",
    "test": "yarn test:unit && yarn lint:js && yarn test:depcheck",
    "test:api": "DEBUG='TEST:DINA*' API_TESTS=true jest --env=node --watch",
    "test:batch": "DEBUG='TEST:DINA*' BATCH_TESTS=true jest --env=node --watch",
    "test:db": "DEBUG='TEST:DINA*' DB_TESTS=true jest --env=node --watch",
    "test:depcheck": "depcheck . --ignores=pg",
    "test:integration": "DEBUG='TEST:DINA*' API_TESTS=true jest --env=node && DEBUG='TEST:DINA*' DB_TESTS=true jest --env=node",
    "test:performance": "DEBUG='DINA:*:performance*' node src/performance/index.js",
    "test:unit": "jest --env=node",
    "test:unit:watch": "jest --env=node --watch"
  },
  "engines": {
    "node": "8.9.1",
    "yarn": "^1.3.2"
  }
}
