  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^22.1.4",
    "nodemon": "^1.14.12",
    "prettier": "1.16.4"
  },
  "scripts": {
    "build:trees": "node ./src/scripts/buildFileStructureTrees.js",
    "lint:js": "eslint 'src/**/*.js'",
    "lint:js:fix": "eslint 'src/**/*.js' --fix",
    "pretty": "prettier --write 'src/**/*.{js,jsx,json}'",
    "production:start:api": "NODE_ENV=production DEBUG='DINA*' node src/apps/production/api/index.js",
    "production:start:worker": "NODE_ENV=production DEBUG='DINA*' node src/apps/production/worker/index.js",
    "start": "WORKER_ROLE=all NODE_ENV=development DEBUG='DINA*,-DINA:LOG_INFO:lib/jobs/worker, -DINA:LOG_DEBUG:controller/jobGetMany' nodemon src/apps/development/api/index.js",
    "start:test": "TEST_SYNC_MODELS=true NODE_ENV=test DEBUG='DINA*' nodemon src/apps/test/api/index.js",
    "start:inspect": "WORKER_ROLE=all NODE_ENV=development DEBUG='DINA*,-DINA:LOG_INFO:lib/jobs/worker' node --inspect src/apps/development/api/index.js",
    "test": "yarn test:unit && yarn lint:js && yarn test:depcheck",
    "test:api": "TEST_API=true DEBUG='TEST:DINA*' jest --env=node --forceExit --runInBand",
    "test:api:watch": "TEST_API=true DEBUG='TEST:DINA*' jest --env=node --watch --runInBand",
    "test:db": "TEST_DB=true  DEBUG='TEST:DINA*' jest --env=node --forceExit --runInBand",
    "test:db:watch": "TEST_DB=true  DEBUG='TEST:DINA*' jest --env=node --forceExit --runInBand --watch",
    "test:depcheck": "depcheck . --ignores='pg,memwatch-next'",
    "test:unit": "TEST_UNIT=true jest --env=node",
    "test:unit:watch": "jest --env=node --watch"
  },
  "engines": {
    "node": "10.15.1",
    "yarn": "1.10.1"
  }
}
