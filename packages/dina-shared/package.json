  "scripts": {
    "lint:js": "eslint 'src/**/*.js'",
    "lint:js:fix": "eslint 'src/**/*.js' --fix",
    "pretty": "prettier --single-quote --write 'src/**/*.{js,jsx}'",
    "start": "DEBUG='DINA*' node src/server/index.js",
    "start:watch": "DEBUG='DINA*' pm2 start src/server/index.js --watch --name postgres-server && pm2 logs",
    "stop:watch": "pm2 stop postgres-server",
    "test": "npm run lint:js",
    "test:api": "API_TESTS=true jest --env=node ./src/__tests__/index.js"
  },
  "dependencies": {
    "ajv": "^5.2.3",
    "json-schema-faker": "^0.4.4",
    "object-path": "^0.11.4",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "prettier": "1.8.2"
  },
  "engines": {
    "node": "8.9.1",
    "yarn": "^1.3.2"
  }
}
