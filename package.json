{
  "name": "dina-collections",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all -p build:ui build:schema build:semantic-ui",
    "build:ui": "cd ./packages/dina-collections-ui && yarn build && yarn build:storybook",
    "build:ui:mock": "cd ./packages/dina-collections-ui && yarn build:mock && yarn build:storybook",
    "build:schema": "cd ./packages/dina-shared && yarn build",
    "build:semantic-ui": "cd ./packages/dina-semantic-ui && yarn build:docs",
    "install": "yarn install:schema && yarn install:semantic-ui && yarn install:ui && yarn install:api && yarn install:shared",
    "install:api": "cd ./packages/dina-collections-api && yarn install",
    "install:ui": "cd ./packages/dina-collections-ui && yarn install",
    "install:schema": "cd ./packages/dina-schema && yarn install",
    "install:semantic-ui": "cd ./packages/dina-semantic-ui && yarn install",
    "install:shared": "cd ./packages/dina-shared && yarn install",
    "setup": "yarn setup:env && yarn install && yarn setup:links && yarn test",
    "setup:env": "./scripts/create-env.sh",
    "setup:links": "yarn setup:links:shared && yarn setup:links:api && yarn setup:links:ui",
    "setup:links:shared": "cd ./packages/dina-shared && yarn link",
    "setup:links:api": "cd ./packages/dina-collections-api && yarn link dina-shared",
    "setup:links:ui": "cd ./packages/dina-collections-ui && yarn link dina-shared",
    "start:api": "cd ./packages/dina-collections-api && yarn start",
    "start:ui": "cd ./packages/dina-collections-ui && yarn start",
    "start:ui:mock": "cd ./packages/dina-collections-ui && yarn start",
    "test": "yarn test:schema && yarn test:api && yarn test:ui && yarn test:shared",
    "test:api": "cd ./packages/dina-collections-api && yarn test",
    "test:ui": "cd ./packages/dina-collections-ui && yarn test:coverage",
    "test:schema": "cd ./packages/dina-schema && yarn test",
    "test:shared": "cd ./packages/dina-shared && yarn test",
    "uninstall": "yarn uninstall:api && yarn uninstall:ui && yarn uninstall:schema && yarn uninstall:semantic-ui && yarn uninstall:shared",
    "uninstall:api": "rm -rf ./packages/dina-collections-api/node_modules",
    "uninstall:ui": "rm -rf ./packages/dina-collections-ui/node_modules",
    "uninstall:schema": "rm -rf ./packages/dina-schema/node_modules",
    "uninstall:semantic-ui": "rm -rf ./packages/dina-semantic-ui/node_modules",
    "uninstall:shared": "rm -rf ./packages/dina-shared/node_modules"
  },
  "contributors": [
    "Anton Ã–berg <oberg.anton@gmail.com>",
    "Fredrik Olovsson <fogithub@gmail.com>"
  ],
  "license": "AGPL-3.0",
  "dependencies": {
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2"
  },
  "engines": {
    "node": "8.9.1",
    "yarn": "^1.3.2"
  }
}
