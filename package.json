{
  "name": "dina-collections",
  "version": "0.24.0-test-2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "yarn build:ui && yarn build:semantic-ui",
    "build:changelog": "auto-changelog -p --tag-pattern '^((?!rc|test).)*$' --template changelog-template.hbs && git add CHANGELOG.md",
    "build:common:schema:api": "cd ./packages/common && yarn build:schema:api",
    "build:common:schema:models": "cd ./packages/common && yarn build:schema:models",
    "build:data:zip": "node ./packages/scripts/src/js/packageJsonDataFiles.js -s local",
    "build:docker:backend": "CI_BUILD_API=true ./packages/scripts/src/bash/ci-build.sh",
    "build:docker:migrations": "CI_BUILD_MIGRATIONS=true ./packages/scripts/src/bash/ci-build.sh",
    "build:docker:style": "CI_BUILD_STYLE=true ./packages/scripts/src/bash/ci-build.sh",
    "build:docker:ui": "CI_BUILD_UI=true ./packages/scripts/src/bash/ci-build.sh",
    "build:docs": "cd ./packages/docs && yarn build",
    "build:elastic-indices": "yarn start:dbs && cd ./packages/migrations && yarn elasticsearch:development:rebuild-indices",
    "build:markdown": "yarn build:trees && cd ./packages/docs && yarn interpolateFiles && yarn build:markdown:cli",
    "build:markdown:cli-docs": "cd ./packages/docs && yarn build:markdown:cli",
    "build:semantic-ui": "cd ./packages/style && yarn build:docs",
    "build:trees": "yarn build:trees:ui && yarn build:trees:common && yarn build:trees:backend",
    "build:trees:backend": "cd ./packages/backend && yarn build:trees",
    "build:trees:common": "cd ./packages/common && yarn build:trees",
    "build:trees:ui": "cd ./packages/ui && yarn build:trees",
    "build:ui": "cd ./packages/ui && yarn build",
    "build:version-info": "node ./packages/scripts/src/js/buildAndCommitVersionInfo.js",
    "cli:docs": "node ./packages/scripts/src/js/scriptDocs --packageJsonPath=./package.json --inputPath=./packages/docs/docs/development/cli.js",
    "delete-test-tags": "./packages/scripts/src/bash/delete-test-tags",
    "exec:local": "node ./packages/scripts/src/js/localExec.js",
    "exec:production": "node ./packages/scripts/src/js/remoteExecProduction.js",
    "exec:stage": "node ./packages/scripts/src/js/remoteExecStage.js",
    "exec:test": "node ./packages/scripts/src/js/remoteExecTest.js",
    "export:data:sample:sql": "yarn import:data:sample && yarn export:data:sql -f ./data/sample.dump.sql",
    "export:data:sql": "./packages/scripts/src/bash/docker-dump-db-to-sql.sh -d dina_dev",
    "export:elastic-indices:json": "yarn start:elasticsearch && ./packages/scripts/src/bash/elasticsearch-export-indices.sh",
    "export:elastic-indices:sample": "yarn import:data:sample && sleep 5 && ./packages/scripts/src/bash/elasticsearch-export-indices.sh -f ./data/sample.index",
    "import:data:json": "yarn start:dbs && cd ./packages/migrations && yarn setup:development",
    "import:data:sample": "yarn start:dbs && yarn exec:local rm-data.sh || true && yarn setup:sample-data && cd ./packages/migrations && yarn setup:development",
    "import:data:sql": "yarn remote:import:data:sql -s local",
    "import:data:zip": "yarn start:dbs && node ./packages/scripts/src/js/unpackDataFiles.js -s local && cd ./packages/migrations && yarn setup:development",
    "import:elastic-indices:json": "yarn start:elasticsearch && ./packages/scripts/src/bash/elasticsearch-import-indices.sh",
    "import:elastic-indices:sample": "yarn start:elasticsearch && ./packages/scripts/src/bash/elasticsearch-import-indices.sh -f ./data/sample.index",
    "install": "yarn install:schema && yarn install:semantic-ui && yarn install:ui && yarn install:backend && yarn install:common && yarn install:scripts && yarn install:migrations && yarn install:docs",
    "install:backend": "cd ./packages/backend && yarn install",
    "install:common": "cd ./packages/common && yarn install",
    "install:docs": "cd ./packages/docs && yarn install",
    "install:migrations": "cd ./packages/migrations && yarn install",
    "install:schema": "cd ./packages/models && yarn install",
    "install:scripts": "cd ./packages/scripts && yarn install",
    "install:semantic-ui": "cd ./packages/style && yarn install",
    "install:ui": "cd ./packages/ui && yarn install",
    "lock-schema": "cd ./packages/common && yarn lock-schema && yarn test:schema-locked",
    "migrate:latest": "cd ./packages/migrations && yarn db:development:migrate:latest",
    "migrate:one": "cd ./packages/migrations && yarn db:development:migrate:one",
    "migrate:undo:one": "cd ./packages/migrations && yarn db:development:migrate:undo:one",
    "postversion": "./packages/scripts/src/bash/release-post-version.sh",
    "pretty": "yarn pretty:schema && yarn pretty:ui && yarn pretty:backend && yarn pretty:common && yarn pretty:scripts && yarn pretty:migrations",
    "pretty:backend": "cd ./packages/backend && yarn pretty",
    "pretty:common": "cd ./packages/common && yarn pretty",
    "pretty:migrations": "cd ./packages/migrations && yarn pretty",
    "pretty:schema": "cd ./packages/models && yarn pretty",
    "pretty:scripts": "cd ./packages/scripts && yarn pretty",
    "pretty:ui": "cd ./packages/ui && yarn pretty",
    "preversion": "./packages/scripts/src/bash/release-pre-version.sh",
    "remote:build:elastic-indices": "node ./packages/scripts/src/js/exec.js docker-rebuild-search.sh",
    "remote:deploy": "node ./packages/scripts/src/js/exec.js docker-deploy.sh",
    "remote:download:data:sql": "node ./packages/scripts/src/js/downloadSqlDump.js",
    "remote:download:data:zip": "node ./packages/scripts/src/js/downloadZippedFiles.js",
    "remote:exec:cmd": "node ./packages/scripts/src/js/execCmd.js",
    "remote:exec:script": "node ./packages/scripts/src/js/exec.js",
    "remote:export:data:sql": "node ./packages/scripts/src/js/exec.js docker-dump-db-to-sql.sh",
    "remote:git:checkout-tag": "node ./packages/scripts/src/js/exec.js git-checkout-tag.sh",
    "remote:import:data:json": "node ./packages/scripts/src/js/exec.js docker-import-data-from-files.sh",
    "remote:import:data:sample": "node ./packages/scripts/src/js/exec.js docker-import-data-from-sample.sh",
    "remote:import:data:sql": "node ./packages/scripts/src/js/importFromSqlDump.js",
    "remote:import:data:zip": "node ./packages/scripts/src/js/importFromZip.js",
    "remote:log": "node ./packages/scripts/src/js/remoteLog.js",
    "remote:migrate:latest": "node ./packages/scripts/src/js/exec.js docker-migrate-latest.sh",
    "remote:migrate:one": "node ./packages/scripts/src/js/exec.js docker-migrate-one.sh",
    "remote:migrate:undo:one": "node ./packages/scripts/src/js/exec.js docker-migrate-undo-one.sh",
    "remote:print:server-status": "node ./packages/scripts/src/js/checkAllServers.js",
    "remote:unpack:zip": "node ./packages/scripts/src/js/unpackDataFiles.js",
    "remote:upload:data:zip": "node ./packages/scripts/src/js/uploadDataZip.js",
    "setup": "yarn setup:env && yarn setup:sample-data && yarn install && yarn setup:links && yarn test",
    "setup:dev": "yarn setup:env:dev && yarn setup:sample-data && yarn install && yarn setup:links && yarn test",
    "setup:env": "./packages/scripts/src/bash/create-env.sh",
    "setup:env:ci:docker": "./packages/scripts/src/bash/create-env-ci-docker.sh",
    "setup:env:ci:local": "./packages/scripts/src/bash/create-env-ci-local.sh",
    "setup:env:dev": "./packages/scripts/src/bash/create-env-development.sh",
    "setup:links": "yarn setup:links:common && yarn setup:links:backend && yarn setup:links:ui && yarn setup:links:scripts && yarn setup:links:migrations",
    "setup:links:backend": "cd ./packages/backend && yarn link && yarn link common",
    "setup:links:common": "cd ./packages/common && yarn link",
    "setup:links:migrations": "cd ./packages/migrations && yarn link backend && yarn link common && yarn link scripts",
    "setup:links:scripts": "cd ./packages/scripts && yarn link",
    "setup:links:ui": "cd ./packages/ui && yarn link common",
    "setup:sample-data": "yarn exec:local create-sample-data.sh",
    "start:backend": "yarn start:dbs && cd ./packages/backend && yarn start",
    "start:dbs": "yarn start:postgres && yarn start:elasticsearch && yarn start:mysql",
    "start:docs": "cd ./packages/docs && yarn start",
    "start:elasticsearch": "docker-compose -f docker-compose.yaml up -d elasticsearch",
    "start:keycloak": "yarn start:mysql && REALM=-dev docker-compose -f docker-compose.yaml up -d keycloak",
    "start:kibana": "docker-compose -f docker-compose.dev-utils.yaml up -d kibana",
    "start:mysql": "docker-compose -f docker-compose.yaml up -d mysql",
    "start:postgres": "docker-compose -f docker-compose.yaml up -d postgres",
    "start:ui": "cd ./packages/ui && yarn start",
    "test": "yarn setup:env && yarn test:models && yarn test:backend:unit && yarn test:ui && yarn test:common",
    "test:all": "yarn test:models && yarn test:backend:unit && yarn test:backend:integration && yarn test:ui && yarn test:common && yarn test:e2e",
    "test:backend:integration": "cd ./packages/backend && yarn test:integration",
    "test:backend:unit": "cd ./packages/backend && yarn test",
    "test:common": "cd ./packages/common && yarn test",
    "test:common:schema-locked": "cd ./packages/common && yarn test:schema-locked",
    "test:e2e": "cd ./packages/ui && yarn test:e2e",
    "test:models": "cd ./packages/models && yarn test",
    "test:ui": "cd ./packages/ui && yarn test:coverage",
    "uninstall": "yarn uninstall:backend && yarn uninstall:ui && yarn uninstall:schema && yarn uninstall:semantic-ui && yarn uninstall:common && yarn uninstall:migrations && yarn uninstall:scripts && yarn uninstall:docs",
    "uninstall:backend": "rm -rf ./packages/backend/node_modules",
    "uninstall:common": "rm -rf ./packages/common/node_modules",
    "uninstall:docs": "rm -rf ./packages/docs/node_modules",
    "uninstall:migrations": "rm -rf ./packages/migrations/node_modules",
    "uninstall:schema": "rm -rf ./packages/models/node_modules",
    "uninstall:scripts": "rm -rf ./packages/scripts/node_modules",
    "uninstall:semantic-ui": "rm -rf ./packages/style/node_modules",
    "uninstall:ui": "rm -rf ./packages/ui/node_modules",
    "version": "./packages/scripts/src/bash/release-version.sh",
    "version:minor": "VERSION_TYPE=release yarn version --minor",
    "version:patch": "VERSION_TYPE=release yarn version --patch",
    "version:rc": "VERSION_TYPE=pre-release yarn version",
    "version:release": "VERSION_TYPE=release yarn version",
    "version:test": "VERSION_TYPE=test-release yarn version"
  },
  "contributors": [
    "Anton Öberg <oberg.anton@gmail.com>",
    "Fredrik Olovsson <fogithub@gmail.com>",
    "Ida Li <ida.li@nrm.se>"
