{
  "name": "dina-collections",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all -p build:ui build:schema build:semantic-ui",
    "build:common": "cd ./packages/common && npm run build",
    "build:schema": "cd ./packages/common && npm run build:schema",
    "build:semantic-ui": "cd ./packages/dina-style && npm run build:docs",
    "build:trees": "npm run build:trees:ui && npm run build:trees:common && npm run build:trees:backend",
    "build:trees:backend": "cd ./packages/backend && npm run build:trees",
    "build:trees:common": "cd ./packages/common && npm run build:trees",
    "build:trees:ui": "cd ./packages/ui && npm run build:trees",
    "build:ui": "cd ./packages/ui && npm run build && npm run build:storybook",
    "deploy:local": "node ./packages/scripts/src/js/localExec.js deploy.sh",
    "deploy:local:latest": "node ./packages/scripts/src/js/localExec.js deploy.sh --includeVersion",
    "deploy:production": "node ./packages/scripts/src/js/remoteExecProduction.js deploy.sh",
    "deploy:production:latest": "node ./packages/scripts/src/js/remoteExecProduction.js deploy.sh --includeVersion",
    "deploy:stage": "node ./packages/scripts/src/js/remoteExecStage.js deploy.sh",
    "deploy:stage:latest": "node ./packages/scripts/src/js/remoteExecStage.js deploy.sh --includeVersion",
    "deploy:test": "node ./packages/scripts/src/js/remoteExecTest.js deploy.sh",
    "deploy:test:latest": "node ./packages/scripts/src/js/remoteExecTest.js deploy.sh --includeVersion",
    "install": "npm run install:schema && npm run install:semantic-ui && npm run install:ui && npm run install:backend && npm run install:shared",
    "install:backend": "cd ./packages/backend && npm install",
    "install:schema": "cd ./packages/models && npm install",
    "install:semantic-ui": "cd ./packages/dina-style && npm install",
    "install:shared": "cd ./packages/common && npm install",
    "install:ui": "cd ./packages/ui && npm install",
    "setup": "npm run setup:env && npm install && npm run setup:links && npm run test",
    "setup:env": "./packages/scripts/src/bash/create-env.sh",
    "setup:links": "npm run setup:links:shared && npm run setup:links:backend && npm run setup:links:ui",
    "setup:links:backend": "cd ./packages/backend && npm link common",
    "setup:links:shared": "cd ./packages/common && npm link",
    "setup:links:ui": "cd ./packages/ui && npm link common",
    "setup:loadTestData": "npm run start:dbs && cd ./packages/backend && npm run data:loadAll",
    "start:backend": "npm run start:dbs && cd ./packages/backend && npm start",
    "start:dbs": "npm run start:postgres && npm run start:elasticsearch",
    "start:elasticsearch": "docker-compose -f docker-compose.yaml up -d elasticsearch",
    "start:keycloak": "npm run start:mysql && docker-compose -f docker-compose.yaml up -d keycloak",
    "start:mysql": "docker-compose -f docker-compose.yaml up -d mysql",
    "start:postgres": "docker-compose -f docker-compose.yaml up -d postgres",
    "start:ui": "cd ./packages/ui && npm start",
    "test": "npm run setup:env && npm run test:schema && npm run test:backend:unit && npm run test:ui && npm run test:shared",
    "test:all": "npm run test:schema && npm run test:backend:unit && npm run test:backend:integration && npm run test:ui && npm run test:shared",
    "test:backend:integration": "cd ./packages/backend && npm run test:integration",
    "test:backend:unit": "cd ./packages/backend && npm run test",
    "test:schema": "cd ./packages/models && npm run test",
    "test:shared": "cd ./packages/common && npm run test",
    "test:ui": "cd ./packages/ui && npm run test:coverage",
    "uninstall": "npm run uninstall:backend && npm run uninstall:ui && npm run uninstall:schema && npm run uninstall:semantic-ui && npm run uninstall:shared",
    "uninstall:backend": "rm -rf ./packages/backend/node_modules",
    "uninstall:schema": "rm -rf ./packages/models/node_modules",
    "uninstall:semantic-ui": "rm -rf ./packages/dina-style/node_modules",
    "uninstall:shared": "rm -rf ./packages/common/node_modules",
    "uninstall:ui": "rm -rf ./packages/ui/node_modules"
  },
  "contributors": [
    "Anton Ã–berg <oberg.anton@gmail.com>",
    "Fredrik Olovsson <fogithub@gmail.com>"
  ],
  "license": "AGPL-3.0",
  "dependencies": {
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2"
  },
  "engines": {
    "node": "8.12.0",
    "npm": "6.4.1"
  }
}
