{
  "name": "dina-collections",
  "version": "0.6.4",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all -p build:ui build:schema build:semantic-ui",
    "build:common": "cd ./packages/common && yarn build",
    "build:schema": "cd ./packages/common && yarn build:schema",
    "build:semantic-ui": "cd ./packages/dina-style && yarn build:docs",
    "build:trees": "yarn build:trees:ui && yarn build:trees:common && yarn build:trees:backend",
    "build:trees:backend": "cd ./packages/backend && yarn build:trees",
    "build:trees:common": "cd ./packages/common && yarn build:trees",
    "build:trees:ui": "cd ./packages/ui && yarn build:trees",
    "build:ui": "cd ./packages/ui && yarn build && yarn build:storybook",
    "deploy:local": "node ./packages/scripts/src/js/localExec.js deploy-docker.sh",
    "deploy:local:latest": "node ./packages/scripts/src/js/localExec.js deploy-docker.sh --includeTag",
    "deploy:production": "node ./packages/scripts/src/js/remoteExecProduction.js deploy-docker.sh",
    "deploy:production:latest": "node ./packages/scripts/src/js/remoteExecProduction.js deploy-docker.sh --includeTag",
    "deploy:stage": "node ./packages/scripts/src/js/remoteExecStage.js deploy-docker.sh",
    "deploy:stage:latest": "node ./packages/scripts/src/js/remoteExecStage.js deploy-docker.sh --includeTag",
    "deploy:test": "node ./packages/scripts/src/js/remoteExecTest.js deploy-docker.sh",
    "deploy:test:latest": "node ./packages/scripts/src/js/remoteExecTest.js deploy.sh --includeTag",
    "exec:production": "node ./packages/scripts/src/js/remoteExecProduction.js",
    "exec:stage": "node ./packages/scripts/src/js/remoteExecStage.js",
    "exec:test": "node ./packages/scripts/src/js/remoteExecTest.js",
    "install": "yarn install:schema && yarn install:semantic-ui && yarn install:ui && yarn install:backend && yarn install:common && yarn install:scripts && yarn install:migrations",
    "install:backend": "cd ./packages/backend && yarn install",
    "install:common": "cd ./packages/common && yarn install",
    "install:migrations": "cd ./packages/migrations && yarn install",
    "install:schema": "cd ./packages/models && yarn install",
    "install:scripts": "cd ./packages/scripts && yarn install",
    "install:semantic-ui": "cd ./packages/dina-style && yarn install",
    "install:ui": "cd ./packages/ui && yarn install",
    "postversion": "git push origin master && git push --tags",
    "preversion": "./packages/scripts/src/bash/ensure-master-is-clean.sh",
    "setup": "yarn setup:env && yarn install && yarn setup:links && yarn test",
    "setup:env": "./packages/scripts/src/bash/create-env.sh",
    "setup:links": "yarn setup:links:common && yarn setup:links:backend && yarn setup:links:ui && yarn setup:links:scripts && yarn setup:links:migrations",
    "setup:links:backend": "cd ./packages/backend && yarn link && yarn link common",
    "setup:links:migrations": "cd ./packages/migrations && yarn link backend && yarn link common && yarn link scripts",
    "setup:links:common": "cd ./packages/common && yarn link",
    "setup:links:ui": "cd ./packages/ui && yarn link common",
    "setup:links:scripts": "cd ./packages/scripts && yarn link",
    "setup:loadTestData": "yarn start:dbs && cd ./packages/backend && yarn data:loadAll",
    "start:backend": "yarn start:dbs && cd ./packages/backend && yarn start",
    "start:dbs": "yarn start:postgres && yarn start:elasticsearch && yarn start:mysql",
    "start:elasticsearch": "docker-compose -f docker-compose.yaml up -d elasticsearch",
    "start:keycloak": "yarn start:mysql && docker-compose -f docker-compose.yaml up -d keycloak",
    "start:mysql": "docker-compose -f docker-compose.yaml up -d mysql",
    "start:postgres": "docker-compose -f docker-compose.yaml up -d postgres",
    "start:ui": "cd ./packages/ui && yarn start",
    "test": "yarn setup:env && yarn test:schema && yarn test:backend:unit && yarn test:ui && yarn test:common",
    "test:all": "yarn test:schema && yarn test:backend:unit && yarn test:backend:integration && yarn test:ui && yarn test:common",
    "test:backend:integration": "cd ./packages/backend && yarn test:integration",
    "test:backend:unit": "cd ./packages/backend && yarn test",
    "test:schema": "cd ./packages/models && yarn test",
    "test:common": "cd ./packages/common && yarn test",
    "test:ui": "cd ./packages/ui && yarn test:coverage",
    "uninstall": "yarn uninstall:backend && yarn uninstall:ui && yarn uninstall:schema && yarn uninstall:semantic-ui && yarn uninstall:common && yarn uninstall:migrations && yarn uninstall:scripts",
    "uninstall:backend": "rm -rf ./packages/backend/node_modules",
    "uninstall:schema": "rm -rf ./packages/models/node_modules",
    "uninstall:semantic-ui": "rm -rf ./packages/dina-style/node_modules",
    "uninstall:common": "rm -rf ./packages/common/node_modules",
    "uninstall:ui": "rm -rf ./packages/ui/node_modules",
    "uninstall:scripts": "rm -rf ./packages/scripts/node_modules",
    "uninstall:migrations": "rm -rf ./packages/migrations/node_modules"
  },
  "contributors": [
    "Anton Ã–berg <oberg.anton@gmail.com>",
    "Fredrik Olovsson <fogithub@gmail.com>"
  ],
  "license": "AGPL-3.0",
  "dependencies": {
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2"
  },
  "engines": {
    "node": "8.9.1",
    "yarn": "^1.10.1"
  }
}
